{
    "version": 2,
    "builds": [{
        "src": "./index.js",
        "use": "@vercel/node"
    }, {
        "src": "./public/**",
        "use": "@vercel/static"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
    }, {
        "src": "/",
        "dest": "public"
    }, {
        "src": "/(.*)",
        "dest": "public/$1"
    }]

}